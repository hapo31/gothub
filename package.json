{
  "name": "github-ext",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run watch",
    "watch:local": "cross-env LOCAL=1 webpack-serve --config ./webpack.config.js",
